    "otp-generator": "^4.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.3",
    "stripe": "^20.2.0",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^5.7.2",
