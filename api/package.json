    "@nestjs/swagger": "^11.2.0",
    "@nestjs/websockets": "^11.1.12",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@sendgrid/client": "^8.1.5",
    "@sendgrid/mail": "^8.1.5",
    "@types/node-telegram-bot-api": "^0.64.9",
    "ai": "^4.3.17",
