{
  "name": "nestjs-starter",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "migrate:local": "dotenv -e .env.local -- npx prisma migrate dev --name new-migration && npx prisma generate",
    "migrate:dev": "dotenv -e .env.development -- npx prisma migrate dev --name init && npx prisma generate",
    "migrate:prod": "dotenv -e .env.production -- npx prisma migrate deploy",
    "migrate:staging": "dotenv -e .env.staging -- npx prisma migrate dev --name init && npx prisma generate",
    "studio:local": "npx dotenv -e .env.local -- npx prisma studio",
    "studio:dev": "npx dotenv -e .env.development -- npx prisma studio",
    "studio:prod": "npx dotenv -e .env.production -- npx prisma studio",
    "studio:staging": "npx dotenv -e .env.staging -- npx prisma studio",
    "reset:dev": "npx dotenv -e .env.development -- npx prisma migrate reset",
    "start": "nest start",
    "start:local": "npx dotenv -e .env.local -- nest start  --watch",
    "start:dev": "npx dotenv -e .env.development -- nest start --watch",
    "start:staging": "npx dotenv -e .env.staging -- nest start --watch",
    "start:prod": "npx dotenv -e .env.production -- node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "debug:dev": "npx dotenv -e .env.development -- node --inspect-brk -r tsconfig-paths/register -r ts-node/register ./node_modules/@nestjs/cli/bin/nest.js start --watch"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.23",
    "@bull-board/api": "^6.16.2",
    "@bull-board/express": "^6.16.2",
    "@google-cloud/storage": "^7.18.0",
    "@nestjs/apollo": "^13.1.0",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/bull": "^11.0.4",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/cache-manager": "^3.0.1",
